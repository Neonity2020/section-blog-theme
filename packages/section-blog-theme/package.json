{
  "name": "section-blog-theme",
  "version": "1.1.5",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/types.d.mts",
  "keywords": [
    "blogging",
    "mdx",
    "nextra",
    "mdx-nextjs",
    "nextra-blog",
    "nextra-markdown-blog",
    "nextra-theme",
    "nextra-markdown",
    "mdx-nextjs"
  ],
  "author": {
    "name": "Rajdeep Singh",
    "email": "officialrajdeepsingh@gmail.com"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ],
      "./Tag": [
        "./dist/tag.d.mts"
      ],
      "./components": [
        "./dist/components.d.mts"
      ],
      "./icons": [
        "./dist/icons.d.mts"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "homepage": "https://section-theme-blog-docs.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendweb3/section-theme-blog.git"
  },
  "bugs": {
    "url": "https://github.com/frontendweb3/section-theme-blog/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/index.d.mts"
    },
    "./Tag": {
      "import": "./dist/src/tag.js",
      "types": "./dist/tag.d.mts"
    },
    "./styles.css": {
      "import": "./dist/styles/globals.css"
    },
    "./components": {
      "import": "./dist/src/components.js",
      "types": "./dist/components.d.mts"
    },
    "./icons": {
      "import": "./dist/src/icons.js",
      "types": "./dist/icons.d.mts"
    }
  },
  "scripts": {
    "clean": "rimraf dist ",
    "dev": "tsup --watch",
    "build": "pnpm run clean && tsup --minify",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "lint": "next lint",
    "test": "jest --watch"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^9.4.1",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/typography": "^0.5.12",
    "@types/node": "20.12.7",
    "@types/react": "18.2.77",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "10.4.19",
    "camelcase": "^8.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "dayjs": "^1.11.10",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.2.1",
    "framer-motion": "^11.0.28",
    "lucide-react": "^0.368.0",
    "next": "14.2.1",
    "next-seo": "^6.5.0",
    "next-themes": "^0.3.0",
    "nextra": "^2.13.4",
    "postcss": "8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-web-share": "^2.0.2",
    "rimraf": "^5.0.5",
    "slugify": "^1.6.6",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.2",
    "typescript": "5.4.5",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.1",
    "flowbite-typography": "^1.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
