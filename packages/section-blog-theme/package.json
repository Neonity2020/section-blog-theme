{
  "name": "section-blog-theme",
  "version": "1.1.0",
  "main": "dist/index.js",
  "types": "dist/types.d.mts",
  "keywords": [
    "blogging",
    "mdx",
    "nextra",
    "mdx-nextjs",
    "nextra-blog",
    "nextra-markdown-blog",
    "nextra-theme",
    "nextra-markdown",
    "mdx-nextjs"
  ],
  "author": {
    "name": "Rajdeep Singh",
    "email": "officialrajdeepsingh@gmail.com"
  },
  "license": "MIT",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ],
      "./Tag": [
        "./dist/tag.d.mts"
      ],
      "./components": [
        "./dist/components.d.mts"
      ],
      "./icons": [
        "./dist/icons.d.mts"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "homepage": "https://section-theme-blog-docs.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendweb3/section-theme-blog.git"
  },
  "bugs": {
    "url": "https://github.com/frontendweb3/section-theme-blog/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/index.d.mts"
    },
    "./Tag": {
      "import": "./dist/src/tag.js",
      "types": "./dist/tag.d.mts"
    },
    "./styles.css": {
      "import": "./dist/styles/globals.css"
    },
    "./components": {
      "import": "./dist/src/components.js",
      "types": "./dist/components.d.mts"
    },
    "./icons": {
      "import": "./dist/src/icons.js",
      "types": "./dist/icons.d.mts"
    }
  },
  "scripts": {
    "clean": "rimraf dist ",
    "dev": "tsup --watch",
    "build": "pnpm run clean && tsup --minify",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "lint": "next lint",
    "test": "jest --watch"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "20.10.4",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.17",
    "autoprefixer": "10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "dayjs": "^1.11.10",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.4",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "next-seo": "^6.4.0",
    "next-themes": "^0.2.1",
    "nextra": "^2.13.2",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-web-share": "^2.0.2",
    "rimraf": "^5.0.5",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.1",
    "typescript": "5.3.3",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "pnpm": {
    "overrides": {
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "zod@<=3.22.2": ">=3.22.3",
      "postcss@<8.4.31": ">=8.4.31"
    }
  },
  "packageManager": "pnpm@8.10.5"
}
