{
  "name": "section-blog-theme",
  "version": "0.1.0",
  "private": false,
  "files": [
    "dist/*"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "Tag": [
        "./dist/Tag.d.ts"
      ]
    }
  },
  "homepage": "https://section-theme-blog-docs.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/frontendweb3/section-theme-blog.git"
  },
  "bugs": {
    "url": "https://github.com/frontendweb3/section-theme-blog/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Tag": {
      "import": "./dist/Tag.js",
      "types": "./dist/Tag.d.ts"
    }
  },
  "scripts": {
    "style": "pnpm tailwindcss -i ./styles/globals.css -o ./dist/output.css --watch",
    "clean": "rm  -R dist && tsup",
    "dev": "tsup --watch && talwind",
    "build": "tsup",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "lint": "next lint"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.2.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "eslint": "^7.32.0",
    "eslint-config-next": "^13.4.1",
    "lodash": "^4.17.21",
    "next": "13.4.2",
    "next-seo": "^6.1.0",
    "next-themes": "^0.2.1",
    "nextra": "^2.8.0",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "tsup": "^6.7.0",
    "typescript": "5.0.4",
    "zustand": "^4.4.1"
  }
}
