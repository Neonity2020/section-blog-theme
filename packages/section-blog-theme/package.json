{
  "name": "section-blog-theme",
  "version": "0.1.0",
  "private": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "Tag": [
        "./dist/Tag.d.ts"
      ]
    }
  },
  "homepage": "https://section-theme-blog-docs.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/frontendweb3/section-theme-blog.git"
  },
  "bugs": {
    "url": "https://github.com/frontendweb3/section-theme-blog/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.esm.js",
      "types": "./dist/src/index.d.ts"
    },
    "./Tag": {
      "import": "./dist/Tag.esm.js",
      "types": "./dist/Tag.d.ts"
    },
    "./styles.css": {
      "import": "./dist/styles/globals.css",
      "require": "./dist/styles/globals.css"
    }
  },
  "scripts": {
    "clean": "rm  -R dist",
    "dev": " tsup --watch",
    "build": " pnpm run clean && tsup --minify",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "lint": "next lint"
  },
  "dependencies": {
    "@orama/orama": "1.0.0-beta.16",
    "@orama/plugin-match-highlight": "1.0.0-beta.16",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@types/node": "20.2.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.15",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "dayjs": "^1.11.9",
    "eslint": "^7.32.0",
    "eslint-config-next": "^13.4.19",
    "lodash": "^4.17.21",
    "lucide-react": "^0.272.0",
    "next": "13.4.2",
    "next-seo": "^6.1.0",
    "next-themes": "^0.2.1",
    "nextra": "^2.12.2",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-web-share": "^2.0.2",
    "slugify": "^1.6.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "tsup": "^6.7.0",
    "typescript": "5.0.4",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "postcss-import": "^15.1.0"
  }
}
